    "version": "0.2.0",
    "configurations": [
        {
            "name": "test-rtee-Generator",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/lib/spatialindex/build/bin/test-rtree-Generator",
            "args": [
                "10000",
                "100",
                ">",
                "d"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "test-rtee-RTreeLoad",
            "type": "cppdbg",
